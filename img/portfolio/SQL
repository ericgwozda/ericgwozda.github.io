OR A.ADDED_BY_C = 'KI576' 
OR A.ADDED_BY_C = 'KI535' 
OR A.ADDED_BY_C = 'KI537'
OR A.ADDED_BY_C = 'KI538' 
OR A.ADDED_BY_C = 'KI533' 
OR A.ADDED_BY_C = 'KI531' 
